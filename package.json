    "bignumber.js": "^9.0.1",
    "chota": "^0.8.0",
    "eventemitter3": "^4.0.7",
    "idb": "^6.0.0",
    "rollup-plugin-copy-merge": "^0.3.5",
    "svelte-chota": "^1.8.3",
    "svelte-i18n": "^3.3.6",
    "tweetnacl": "^1.0.3"
  }
