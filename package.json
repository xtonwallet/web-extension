  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint . --ext .svelte --ext .js --fix",
    "build": "npm run dist && gulp build",
    "dist": "rollup --config",
    "start": "rollup --config --watch",
    "test": "mocha --recursive --timeout 30000 tests/e2e/",
    "test-firstrun": "mocha --recursive --timeout 30000 tests/e2e/firstrun/*.js",
    "test-dapps": "mocha --recursive --timeout 30000 tests/e2e/dapp/*.js",
    "version": "gulp patchVersion && auto-changelog -p -b 20 -l 20 -t keepachangelog --starting-version 2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xtonwallet/web-extension.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^14.0.1",
    "@rollup/plugin-replace": "^2.4.2",
    "auto-changelog": "^2.2.1",
    "chromedriver": "^87.0.4",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "globby": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^8.0.0",
    "gulp-json-editor": "^2.5.4",
    "gulp-zip": "^4.0.0",
    "install": "^0.13.0",
    "mocha": "^9.2.2",
    "node-sass": "^7.0",
    "npm": "^8.3.0",
    "postcss": "^8.2.15",
    "rollup": "^2.38.5",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-watch-assets": "^1.0.1",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "svelte": "^3.32.3",
    "svelte-preprocess": "^4.6.9"
